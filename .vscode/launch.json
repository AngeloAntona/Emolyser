{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "OBJC_DISABLE_INITIALIZE_FORK_SAFETY": "YES"
            }
        }
    ]
}